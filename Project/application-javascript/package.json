{
  "name": "asset-transfer-basic",
  "version": "1.0.0",
  "description": "Asset-transfer-basic application implemented in JavaScript",
  "engines": {
    "node": ">=14.14",
    "npm": ">=6"
  },
  "scripts": {
    "lint": "eslint *.js",
    "pretest": "npm run lint",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev-server": "cd server && nodemon --watch . -e js,ts --exec \"npm start\""
  },
  "engineStrict": true,
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2",
    "express-generator": "^4.2.0",
    "fabric-ca-client": "^2.2.19",
    "fabric-network": "^2.2.19",
    "nodemon": "^3.0.2",
    "react-select": "^5.8.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "eslint": "^7.32.0"
  }
}
